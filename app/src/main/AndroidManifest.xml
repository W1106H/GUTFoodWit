<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edwin.loopviewpager">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.gutfoodwit.base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.gutfoodwit.activity.ShoppingCartActivity"></activity>
        <activity android:name="com.example.gutfoodwit.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.gutfoodwit.activity.HomePageActivity"
            android:launchMode="singleInstance"></activity>
    </application>

</manifest>